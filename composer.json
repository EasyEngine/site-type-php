{
    "name": "easyengine/site-type-php",
    "description": "EasyEngine package for PHP site creation.",
    "type": "ee-cli-package",
    "homepage": "https://github.com/easyengine/site-type-php",
    "license": "MIT",
    "authors": [],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "EE\\Site\\Type\\": "src/"
        },
        "files": [ "site-type-php.php" ]
    },
    "require-dev": {
        "wp-cli/mustangostang-spyc": "^0.6.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "bundled": true,
        "commands": [
            "site create --type=php",
            "site delete",
            "site update",
            "site info --type=php",
            "site enable",
            "site disable",
            "site info",
            "site ssl",
            "site list",
            "site reload --type=php",
            "site restart --type=php",
            "site share",
            "site clean"
        ]
    }
}
